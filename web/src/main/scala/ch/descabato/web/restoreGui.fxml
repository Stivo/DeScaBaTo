<StackPane prefHeight="500.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="ch.descabato.web.RestoreController">
    <children>
      <TabPane fx:id="tabs">
            <tabs>
                <Tab text="Browse Files">
                    <content>
                        <fx:include fx:id="browser" source="browserPage.fxml"/>
                    </content>
                </Tab>
                <Tab text="Preview">
                    <content>
                        <fx:include fx:id="preview" source="previewPage.fxml"/>
                    </content>
                </Tab>
            </tabs>
      </TabPane>
    </children>
</StackPane>
